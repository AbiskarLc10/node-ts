{
  "extends":"./tsconfig.options.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "./src",
    "outDir": "./dist",
    "paths": {
      "@auth/*": ["./src/services/auth-service/src/*"],
      "@config/*": ["./src/shared/config/*"],
      "@db/*": ["./src/shared/db/*"]
    },
    "tsBuildInfoFile": "dist/.tsbuildinfo"
  },
  "files": ["./src/index.ts"],
  "references": [
    {
      "path": "./src/shared"
    },
    {
      "path": "./src/services/auth-service"
    },
  ]
}
